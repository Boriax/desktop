#include <iostream>
using namespace std;

int main()
{
     int n=10;
     for(int l=1; l<n;l++){
        int r=0;
        for(int i=1; i<l-1;i++)
            for(int j=i+1; j<l;j++)
                for(int k=1; k<=j;k++)
                    r++;

        std::cout << r << std::endl;
    }
    return 0;
}
